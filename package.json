{
  "name": "verify-contract",
  "version": "1.0.0",
  "description": "This service verifies source code of a deployed contract on Aura Network's chains",
  "main": ".eslintrc.js",
  "author": {
    "name": "Aura Network",
    "email": "admin@aura.network"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aura-nw/verify-contract.git"
  },
  "bugs": {
    "url": "https://github.com/aura-nw/verify-contract/issues"
  },
  "homepage": "https://github.com/aura-nw/verify-contract#readme",
  "dependencies": {
    "@bull-board/api": "^4.6.4",
    "@bull-board/express": "^4.6.4",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1261.0",
    "axios": "^0.21.2",
    "bull": "^4.10.1",
    "dockerode": "^3.3.4",
    "fs": "0.0.1-security",
    "mysql2": "^2.3.3",
    "redis": "^4.5.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.10",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@nestjs/swagger": "^6.1.3",
    "@types/bull": "^4.10.0",
    "@types/dockerode": "^3.3.14",
    "@types/node": "^18.11.9",
    "class-validator": "^0.13.2",
    "rxjs": "^7.5.7"
  }
}
